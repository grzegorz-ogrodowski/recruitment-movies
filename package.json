{
  "name": "movies-api",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/src/server.js",
    "start-tests": "npm run build && node dist/src/server.js test/db.json",
    "test": "mocha"
  },
  "dependencies": {
    "amqp": "0.2.7",
    "body-parser": "1.19.0",
    "enum": "~2.5.0",
    "express": "4.17.1",
    "joi": "14.3.1",
    "lodash": "4.17.15",
    "lodash.combinations": "18.9.19",
    "mocha": "7.1.1",
    "axios": "0.19.2",
    "chai": "4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
